      PROCEDURE VIRTUAL FreeDynTab()
      
      // Attachments for all the messages
      i is int
      FOR i = 1 _TO_ Dimension(:m_parrRubriques)
      	:m_parrRubriques[i]:FreeDynTab()
      END
