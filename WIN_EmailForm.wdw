     procedure_id : 1510356551439082246
     type_code : 14
     code : |1+
      PROCEDURE FillListEmails()
      
      // List of possible recipients read in the "Contacts" folder of Outlook
      
      arrContacts is array of cContact
      clContactList is object cContactList()
      
      ListDeleteAll(EDT_Recipient)
      ListDeleteAll(EDT_RecipientCopy)
      ListDeleteAll(EDT_RecipientBlindCopy)
      
      clContactList:LoadItems()
      
      //Assigns the global array to perform the Databinding on this array
      arrContacts = clContactList.m_parrRubriques
      FOR ALL ELEMENT AContact of arrContacts
      	
      	ListAdd(EDT_Recipient,AContact:m_sEmail)
      	ListAdd(EDT_RecipientCopy,AContact:m_sEmail)
      	ListAdd(EDT_RecipientBlindCopy,AContact:m_sEmail)
      END
      
      
      
      
