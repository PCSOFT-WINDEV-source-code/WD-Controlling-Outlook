     code : |1+
      PROCEDURE WIN_EmailForm(gclMail is cMail, sMode=CST_MODIFICATION,sType=cListEntities::CST_ANSWER)
      
      // List of possible recipients read in the "Contacts" folder of Outlook
      FillListEmails()
      
      IF sMode~=CST_MODIFICATION THEN
      	
      	// Display the incoming message
      	EDT_MessageID=gclMail:m_sID
      	EDT_ReceiveDate=gclMail:m_sReceiveDate
      	EDT_ReceptionTime=gclMail:m_sReceptionTime
      	EDT_Sender=gclMail:m_sSender
      	EDT_Message=	CR+CR+<§$0000§>+CR+...
      	<§$0001§>+gclMail:m_sSender+CR+...
      	<§$0002§>+DateToString(gclMail:m_sReceiveDate, maskSystemDate)+" "+TimeToString(gclMail:m_sReceptionTime)[[1 TO 5]]+CR+...
      	<§$0003§>+gclMail:m_sSubject+CR+CR+gclMail:m_sContent
      	SWITCH gclMail:m_nNbATT
      		CASE 0 : BTN_ATT..Caption=<§$0004§>
      		CASE 1 : BTN_ATT..Caption=<§$0005§>
      		OTHER CASE : BTN_ATT..Caption=StringBuild(<§$0006§>, gclMail:m_nNbATT)
      	END
      	SWITCH Upper(NoSpace(sType))
      		CASE cListEntities::CST_ANSWER
      			STC_Title=<§$0007§>
      			EDT_Object=<§$0008§>+gclMail:m_sSubject
      			EDT_Recipient=gclMail:m_sSender
      		CASE cListEntities::CST_ANSWER_TO_ALL
      			STC_Title=<§$0009§>
      			EDT_Object=<§$0008§>+gclMail:m_sSubject
      			EDT_Recipient=gclMail:m_sSender
      			IF NOT gclMail:m_sDest~="" THEN EDT_Recipient+="; "+gclMail:m_sDest
      		CASE cListEntities::CST_FORWARD
      			STC_Title=<§$000a§>
      			EDT_Object=<§$000b§>+gclMail:m_sSubject
      	END
      	ScreenFirst(EDT_Message)
      ELSE
      	// Default values
      	EDT_Sender = gclMail:m_sUser
      	EDT_ReceiveDate..Caption=<§$000c§>
      	EDT_ReceiveDate=DateSys() 
      	EDT_ReceptionTime=TimeSys()
      	// Clear the attachments
      	gclMail:InitialiseATTCH()
      	BTN_ATT..Caption=<§$0004§>
      END
      
